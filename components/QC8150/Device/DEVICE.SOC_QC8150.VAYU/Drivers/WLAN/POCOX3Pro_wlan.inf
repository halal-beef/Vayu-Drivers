[QcWlan.ndi.NTARM64]
Include            = netvwifibus.inf
Needs              = VWiFiBus.CopyFiles
*IfType            = 71		; IF_TYPE_IEEE80211
*MediaType         = 16		; NdisMediumNative802_11
*PhysicalMediaType = 9		; NdisPhysicalMediumNative802_11
Characteristics    = 0x84    ; NCF_PHYSICAL | NCF_HAS_UI
BusType            = 15        ; PnP Bus
AddReg             = QcWlan.Reg, QcWlan11n.reg, D0PC.reg, wowenable.reg, TxBf.reg, MAS.reg, DriverMode.Reg, enableVht2G.reg, rsc.reg, lso.reg, fwVersion.Reg, caps.Reg, Autologger.reg
CopyFiles          = QcWlan.CopyFiles, bdwlanFiles, WlanFWFiles
AddProperty        = WDTFSOCDeviceCategory

; WLAN Device category for SoCs (WLAN is of type NORMAL = 0)
[WDTFSOCDeviceCategory]
{5D078032-6378-437B-8DA5-9B32B7CA3FDD},2,7,,0

[QcWlan.ndi.NTARM64.Services]
Include         = netvwifibus.inf
Needs           = VWiFiBus.Services
AddService      = qcwlan, 2, QcWlan.Service, QcWlan.EventLog

[QcWlan.ndi.NTARM64.HW]
Include         = netvwifibus.inf
Needs           = VWiFiBus.PnPFilterRegistration.HW

[QcWlan.NMODEM.ndi.NTARM64]
Include            = netvwifibus.inf
Needs              = VWiFiBus.CopyFiles
*IfType            = 71		; IF_TYPE_IEEE80211
*MediaType         = 16		; NdisMediumNative802_11
*PhysicalMediaType = 9		; NdisPhysicalMediumNative802_11
Characteristics    = 0x84    ; NCF_PHYSICAL | NCF_HAS_UI
BusType            = 15        ; PnP Bus
AddReg             = QcWlan.Reg, QcWlan11n.reg, D0PC.reg, wowenable.reg, TxBf.reg, MAS.reg, DriverMode.Reg, enableVht2G.reg, rsc.reg, lso.reg, fwVersion.Reg, caps.Reg, Autologger.reg, modemconcern.reg
CopyFiles          = QcWlan.CopyFiles, bdwlanFiles, WlanFWFiles
AddProperty        = WDTFSOCDeviceCategory

[QcWlan.NMODEM.ndi.NTARM64.Services]
Include         = netvwifibus.inf
Needs           = VWiFiBus.Services
AddService      = qcwlan, 2, QcWlan.Service, QcWlan.EventLog

[QcWlan.NMODEM.ndi.NTARM64.HW]
Include         = netvwifibus.inf
Needs           = VWiFiBus.PnPFilterRegistration.HW

;--------------------------------------------------------------------------
; INTERFACE SECTION
;--------------------------------------------------------------------------
[QcWlan.ndi.NTARM64.Interfaces]
AddInterface=%wlanInterfaceGuid%,,QcWlan.I.addPath

[QcWlan.NMODEM.ndi.NTARM64.Interfaces]
AddInterface=%wlanInterfaceGuid%,,QcWlan.I.addPath

[QcWlan.I.addPath]
AddReg=QcWlan.addPath.AddReg

[QcWlan.addPath.AddReg]
HKR,,Local,,%13%\WLANMDSP.MBN

;-----------------------------------------------------------------------------
; Wireless Local Area Network Adaptor Common
;-----------------------------------------------------------------------------
[QcWlan.Reg]
HKR, Ndi\Interfaces, UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces, LowerRange, 0, "wlan,ethernet,vwifi"
HKR, Ndi\Interfaces, DefUpper,   0, "ndis5"
HKR, Ndi\Interfaces, DefLower,   0, "ethernet"
HKR, Ndi,            Service,    0, %QcWlan.Service.Name%

;-----------------------------------------------------------------------------
;Source
;-----------------------------------------------------------------------------
[SourceDisksNames]
;
; diskid = description[, [tagfile] [, <unused>, subdir]]
;
1 = %DiskId1%,,


[SourceDisksFiles]

wlanmdsp.mbn = 1,


data.msc = 1,
bdwlan.bin = 1,

qcwlan8150.sys=1

[SourceDisksFiles.ARM64]
qcwlan8150.sys=1

;-----------------------------------------------------------------------------
; DestinationDirs
;-----------------------------------------------------------------------------
[DestinationDirs]
QcWlan.CopyFiles = 13
bdwlanFiles = 13
WlanFWFiles = 13

[QcWlan.CopyFiles]
qcwlan8150.sys,,,2

[WlanFWFiles]

wlanmdsp.mbn


[bdwlanFiles]

data.msc
bdwlan.bin




;-----------------------------------------------------------------------------
; Driver and Service Section
;-----------------------------------------------------------------------------

[QcWlan.Service]
DisplayName     = %QcWlan.Service.DispName%
ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 3 ;%SERVICE_DEMAND_START%
ErrorControl    = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary   = %13%\qcwlan8150.sys
LoadOrderGroup  = NDIS


